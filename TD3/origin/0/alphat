FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    floor
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    ceiling
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    sWall
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    nWall
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    sideWalls
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1.94e-05;
    }
    glass1
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    glass2
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    sun
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    heatsource1
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    heatsource2
    {
        type            compressible::alphatWallFunction;
        value           uniform 0;
    }
    Table_master
    {
        type            compressible::alphatWallFunction;
        coupleGroup     Table;
        value           uniform 0;
    }
    Table_slave
    {
        type            compressible::alphatWallFunction;
        coupleGroup     Table;
        value           uniform 0;
    }
}


