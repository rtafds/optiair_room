FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    ".*"
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
    sideWalls
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;  //5 %
        value           uniform 1;
    }
    heatsource2
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
    heatsource1
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        inletensity     0.05;  //5 %
        value           uniform 1;
    }
}


